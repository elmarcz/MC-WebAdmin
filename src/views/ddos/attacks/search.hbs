<div class="concontainer">
  <div class="container">
    <h1>{{title}}</h1>
    <form method="POST" action="{{action}}">
    {{#if id}}
      <div class="form-group">
        <input
          type="text"
          id="username"
          name="id"required
          placeholder="Enter The ID "
        />
      </div>
    {{/if}}
    {{#if date}}
      <div class="form-group">
        <input
          type="date"
          id="username"
          name="date"required
          placeholder="Enter The Date "
        />
      </div>
    {{/if}}
    {{#if url}}
      <div class="form-group">
        <input
          type="text"
          id="username"required
          name="url"
          placeholder="Enter the Url"
        />
      </div>
    {{/if}}
    {{#if publicip}}
      <div class="form-group">
        <input
          type="text"
          id="username"
          name="publicip"required
          placeholder="Enter the public IP"
        />
      </div>
    {{/if}}
        {{#if privateip}}
      <div class="form-group">
        <input
          type="text"
          id="username"
          name="privateip"required
          placeholder="Enter the private IP"
        />
      </div>
    {{/if}}
      {{#if country}}
      <div class="form-group">
        <input
          type="text"
          id="username"
          name="country"
          required
          placeholder="Enter the country name"
        />
      </div>
    {{/if}}
      <button type="submit">Search</button>
    </form>
  </div>
</div>

<style>body{overflow: hidden;}
  body { user-select:all; background-color: #000; color: #fff; font-family: Arial, sans-serif;
  width: 100vw; height: 100vh; } .container { width: 400px; color: #000; margin:
  0 auto; padding: 40px; /*background-color: #fff;*/ background: hsl(218deg 50%
  91%); border-radius: 4px; text-align: center; } h1 { margin-bottom: 20px; }
  .form-group { margin-bottom: 20px; text-align: left; } input[type="text"]{
  width: 382px !important; padding: 8px; border: 1px solid #ccc; border-radius:
  4px; } button[type="submit"] { width: 90%; height:40px; background-color:
  #000; color: #fff; border: none; border-radius: 4px; cursor: pointer; }
  button[type="submit"]:hover { background-color: #333; }
</style>

